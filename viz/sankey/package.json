{
  "dsccViz": {
    "gcsDevBucket": "gs://yulanlin-cv-dev/sankey",
    "gcsProdBucket": "gs://public-community-viz-showcase/sankey",
    "jsFile": "index.js",
    "jsonFile": "index.json",
    "cssFile": "index.css",
    "print": "printMessage.js"
  },
  "scripts": {
    "build:dev": "ts-node scripts/build.ts dev",
    "build:prod": "ts-node scripts/build.ts prod",
    "push:dev": "dscc-scripts viz push -d dev",
    "push:prod": "dscc-scripts viz push -d prod",
    "update_message": "dscc-scripts viz update_message -f object",
    "test": "jest",
    "prettier": "prettier --write \"{src,test}/**/*.{ts,js}\""
  },
  "devDependencies": {
    "@google/dscc": "^0.3.8",
    "@google/dscc-scripts": "^1.0.6"
  },
  "dependencies": {
    "d3": "^5.9.2",
    "d3-sankey": "^0.12.1",
    "d3-scale-chromatic": "^1.3.3",
    "d3-sankey": "^0.12.1",
    "d3-scale-chromatic": "^1.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
